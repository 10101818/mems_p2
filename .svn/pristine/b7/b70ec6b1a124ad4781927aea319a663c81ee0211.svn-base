<script>
    function CheckAccessory(){
        var flag=true;
        $(".tips").html('');

        if(dispatchReportVue.ReportAccessory.Name == undefined || dispatchReportVue.ReportAccessory.Name.trim() == ""){
            $("#nameMSG").html('名称不能为空');
            $("#name").focus();
            flag = false;
        }

        if(dispatchReportVue.ReportAccessory.Source.ID == 0){
            $("#sourceMSG").html('来源不能为空');
            $("#source").focus();
            flag = false;
        }

        if(dispatchReportVue.ReportAccessory.Source.ID != '@stock_Source'){
            if ($("#supplier").val() == '') {
                $("#supplierMSG").html('供应商不能为空');
                $("#supplier").focus();
                flag = false;
            }else if(dispatchReportVue.ReportAccessory.Supplier==null || dispatchReportVue.ReportAccessory.Supplier.ID==0 || dispatchReportVue.ReportAccessory.Supplier.ID==null){
                $("#supplierMSG").html('供应商选择不存在');
                $("#supplier").focus();
                flag = false;
            }
        }

        if(dispatchReportVue.ReportAccessory.NewSerialCode == undefined || dispatchReportVue.ReportAccessory.NewSerialCode.trim() == ""){
            $("#newMSG").html('新装编号不能为空');
            $("#new").focus();
            flag = false;
        }
        //else if(!/^[A-Za-z0-9]{0,10}$/.test(dispatchReportVue.ReportAccessory.NewSerialCode)){
        //    $("#newMSG").html('新装编号格式错误');
        //    $("#new").focus();
        //    flag = false;
        //}

        if(dispatchReportVue.ReportAccessory.Amount == undefined || dispatchReportVue.ReportAccessory.Amount.trim() == ""){
            $("#amountMSG").html('价格不能为空');
            $("#amount").focus();
            flag = false;
        }
        else if ($("#amount").val() > 99999999.99){
            $("#amountMSG").html('金额最大为99999999.99');
            $("#amount").focus();
            flag = false;
        }else if(!/^([1-9]\d{0,9}|0)([.]?|(\.\d{1,2})?)$/.test(dispatchReportVue.ReportAccessory.Amount)){
            $("#amountMSG").html('格式错误');
            $("#amount").focus();
            flag = false;
        }

        if(dispatchReportVue.ReportAccessory.Qty == undefined || dispatchReportVue.ReportAccessory.Qty == 0 || dispatchReportVue.ReportAccessory.Qty.trim() == ""){
            $("#qtyMSG").html('数量不能为空');
            $("#qty").focus();
            flag = false;
        }
        else if(dispatchReportVue.ReportAccessory.Qty > 999999999){
            $("#qtyMSG").html('数量最大为999999999');
            $("#qty").focus();
            flag = false;
        }
        else if (!/^([1-9]\d{0,8}|0)$/.test(dispatchReportVue.ReportAccessory.Qty)) {
            $("#qtyMSG").html('格式错误');
            $("#qty").focus();
            flag = false;
        }

        if(dispatchReportVue.ReportAccessory.OldSerialCode == undefined || dispatchReportVue.ReportAccessory.OldSerialCode.trim() == ""){
            $("#oldMSG").html('拆下编号不能为空');
            $("#old").focus();
            flag = false;
        }
        //else if(!/^[A-Za-z0-9]{0,10}$/.test(dispatchReportVue.ReportAccessory.OldSerialCode)){
        //    $("#oldMSG").html('格式错误');
        //    $("#old").focus();
        //    flag = false;
        //}

        return flag;
    }
</script>
