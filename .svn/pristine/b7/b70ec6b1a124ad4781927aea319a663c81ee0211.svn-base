                        </td>
                        <td>
                            <input type="text" readonly class="linkFile" v-bind:id="'FileNewReportAccessory'+index" v-on:click="OpenLocalFile('FileNewReportAccessory'+index,'@BusinessObjects.Domain.ObjectTypes.ReportAccessory')" />
