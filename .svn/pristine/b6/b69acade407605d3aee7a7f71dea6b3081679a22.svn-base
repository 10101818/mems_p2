		$(".requiredItem").html("*");
		$(".unrequiredItem").html("&nbsp;&nbsp;");
		$(".requiredItem").css("color", "red");
	});
	var options = {};
	$(function () {
		$(".btn-newFujiClassI").click(function () {
			$("#FujiClassIModalCenter").modal(options);
		})
		$(".btn-editFujiClassI").click(function () {
