						<div class="item-body">
							<div class="icon-container waves-effect waves-purple" >
								<i class="material-icons icon-sm">add_to_photos</i>
							</div>
							<div class="text-footer">新增</div>
						</div>
					</div>
					<div class="col s4 center">
						<div class="item-body">
							<div class="icon-container waves-effect waves-purple" >
								<i class="material-icons icon-sm">insert_chart</i>
							</div>
							<div class="text-footer">生命周期管理</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!--JavaScript at end of body for optimized loading-->
    <script type="text/javascript" src="js/materialize.min.js"></script>
