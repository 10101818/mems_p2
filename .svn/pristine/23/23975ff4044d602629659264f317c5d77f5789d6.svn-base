		$(".addConsumable").click(function () {
			$("#AddConsumable").modal(options);
		})
	})

	$(function () {
		$(".requiredItem").html("*");
		$(".unrequiredItem").html("&nbsp;&nbsp;");
		$(".requiredItem").css("color", "red");
	});
	$("#checkedStatus").click(function () {
		var s = $("#checkedStatus").is(":checked");
		if (s) {
			$(".ystime").css('display', "block");
			$("#uncheckedStatus").removeAttr("checked");
		}
	})

	$("#uncheckedStatus").click(function () {
		var s = $("#uncheckedStatus").is(":checked");
		if (s) {
			$("#checkedStatus").removeAttr("checked");
			$(".ystime").css("display", "none");
		}
	})
</script>

