<body>
	
	<div class="limiter">
		<div class="container-login100" style="background-image: url('images/bg_01.jpg');">
			<div class="wrap-login100">
				<div class="login100-form validate-form">
					<span class="login100-form-logo">
						<img src="images/logo_horizon.png" style="width: 250%;"/>
					</span>

					<span class="login100-form-title p-b-34 p-t-27">
							医疗设备管理系统
					</span>

					<div class="wrap-input100">
					<table>
						<tr>
							<td width="300px"> 
								<input class="input100" disabled type="text" name="pass" placeholder="用户角色">
								<span class="focus-input100" data-placeholder="&#xf207;"></span>
							</td>
							<td>
								<form name="user">
									<select name="user1" style="background: none; border: 0rem;color: white;">
										<option style="color: black" value="page/index-FujiManager.html">超级管理员</option>
										<option style="color: black" value="page/index-Engineer.html">管理员</option>
										<option style="color: black" value="index.html">超级用户</option>
									</select>
								</form>
							</td>
						</tr>
					</table>
						
						
					</div>

					<div class="wrap-input100">
						<input class="input100" type="password" name="pass" placeholder="密码">
						<span class="focus-input100" data-placeholder="&#xf191;"></span>
					</div>

					<div class="contact100-form-checkbox">
						<input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
						<label class="label-checkbox100" for="ckb1">
							记住密码
						</label>
					</div>

					<div class="container-login100-form-btn">
						<button class="login100-form-btn" onclick="javascript:location.href=document.user.user1.options[document.user.user1.selectedIndex].value;">
							登录	
						</button>
					</div>

					<div class="text-center p-t-90">
						<a class="txt1" href="#">
							忘记密码?
						</a>
					</div>
				</div>
			</div>
		</div>
	</div>

	

	<div id="dropDownSelect1"></div>
	
