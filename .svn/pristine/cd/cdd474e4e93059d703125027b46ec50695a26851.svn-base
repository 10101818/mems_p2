<script>
	$(function () {
		InitConditionsHide();
		InitResultHide();
		InitBtn();
		$("#Premise").show();
		$("#Pricing").show();
		var options = {};
		$(".GoToPremise").click(function () {
			InitConditionsHide();
			$("#Premise").show();
		})
		$(".GoToEquipmentList").click(function () {
			$("#EquipmentList").modal(options);
		})
		$(".GoToInvSpareList").click(function () {
			$("#InvSpareList").modal(options);
		})
		$(".GoToCoverSupplies").click(function () {
			$("#CoverSupplies").modal(options);
		})
		$(".GoToCTList").click(function () {
			$("#CTList").modal(options);
		})
		$(".GoToImportantEqpt").click(function () {
			$("#ImportantEqpt").modal(options);
		})
		$(".GoToGeneralEqpt").click(function () {
			$("#GeneralEqpt").modal(options);
		})
		$(".GoToLabourCost").click(function () {
			$("#LabourCost").modal(options);
		})
		$(".btn").click(function () {
			InitBtn();
			$(this).addClass('activeBtn');
		})

		$(".pricing").click(function () {
			InitResultHide();
			$("#Pricing").show();

		})
		$(".costDetail").click(function () {
			InitResultHide();
			$("#CostDetail").show();
		})
		$(".accessibility").click(function () {
			InitResultHide();
			$("#Accessibility").show();
		})
	})

	function InitBtn() {
		$('.btn').removeClass('activeBtn');
	}
	function InitConditionsHide() {
		$("#Premise").hide();
		$("#LabourCost").hide();
		$("#EquipmentList").hide();
		$("#InvSpareList").hide();
		$("#CoverSupplies").hide();
		$("#CTList").hide();
		$("#ImportantEqpt").hide();
		$("#GeneralEqpt").hide();
	}
	function InitResultHide() {
		$("#Pricing").hide();
		$("#CostDetail").hide();
		$("#Accessibility").hide();
	}
</script>
